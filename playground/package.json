{
  "name": "playground",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.0",
    "vue": "^3.3.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@wtto00/unocss-preset-theme": "workspace:*",
    "typescript": "^5.2.2",
    "unocss": "^0.58.0",
    "unocss-preset-theme": "workspace:*",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,mjs,jsx,ts,mts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{ts,mts,tsx,vue}": [
      "pnpm -F playground vue-tsc --noEmit"
    ]
  }
}
