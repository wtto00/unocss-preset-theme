{
  "name": "@wtto00/unocss-preset-theme",
  "type": "module",
  "version": "0.12.0",
  "packageManager": "pnpm@9.4.0",
  "description": "dynamic theme",
  "author": "Dunqing <dengqing0821@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wtto00/unocss-preset-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wtto00/unocss-preset-theme.git"
  },
  "bugs": {
    "url": "https://github.com/wtto00/unocss-preset-theme/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-theme"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.css",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "play": "pnpm --filter ./playground run dev",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "release": "bumpp package.json packages/**/package.json",
    "format": "prettier . --write --ignore-unknown",
    "prepare": "husky install && husky set .husky/pre-commit \"npx lint-staged\"",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "@unocss/core": ">= 0.52.0"
  },
  "dependencies": {
    "@unocss/core": "^0.61.0",
    "@unocss/rule-utils": "^0.61.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@types/node": "^20.14.9",
    "@unocss/preset-uno": "^0.61.0",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,mjs,jsx,ts,mts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{ts,mts,tsx}": [
      "pnpm -F @wtto00/unocss-preset-theme tsc --noEmit"
    ]
  }
}
