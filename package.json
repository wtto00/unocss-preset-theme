{
  "name": "@wtto00/unocss-preset-theme",
  "type": "module",
  "version": "0.11.1",
  "packageManager": "pnpm@8.9.2",
  "description": "Dynamic theme for unocss.",
  "author": "Dunqing <dengqing0821@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Dunqing/unocss-preset-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunqing/unocss-preset-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Dunqing/unocss-preset-theme/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-theme"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.css",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "play": "pnpm --filter ./playground run dev",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "release": "bumpp package.json packages/**/package.json"
  },
  "peerDependencies": {
    "@unocss/core": ">= 0.52.0"
  },
  "dependencies": {
    "@unocss/core": "^0.57.1",
    "@unocss/rule-utils": "^0.57.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.29",
    "@types/node": "^20.8.9",
    "@unocss/preset-uno": "^0.57.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  }
}
